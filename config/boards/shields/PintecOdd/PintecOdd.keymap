#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/matrix_transform.h>
#include <dt-bindings/zmk/gpio.h>  // Include GPIO binding for key actions

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
            >;
        };

        lower_layer {
            bindings = <
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
                &gpio_on    &gpio_on    &gpio_on    &gpio_on    &gpio_on   &gpio_on
            >;
        };
    };
};
